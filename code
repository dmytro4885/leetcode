#include <queue>

class MyStack {
private:
    std::queue<int> q1, q2;

public:
    MyStack() { 
    }
    void push(int x) {
        q1.push(x);
    }
    
    int pop() {
        while (q1.size() > 1) {
            q2.push(q1.front());
            q1.pop();
        }
        int top_element = q1.front();
        q1.pop();
        std::swap(q1, q2);
        return top_element;
    }
    
    int top() {
        while (q1.size() > 1) {
            q2.push(q1.front());
            q1.pop();
        }
        int top_element = q1.front();
        q2.push(top_element);
        q1.pop();
        std::swap(q1, q2);
        return top_element;
    }
    
    bool empty() {
        return q1.empty() && q2.empty();
    }
};
/**
 * Your MyStack object will be instantiated and called as such:
 * MyStack* obj = new MyStack();
 * obj->push(x);
 * int param_2 = obj->pop();
 * int param_3 = obj->top();
 * bool param_4 = obj->empty();
 */
